def find_smallest_number(numbers):
    # Initialize smallest_number to the first element in the list
    smallest_number = numbers[0]
    
    # Loop through the list starting from the second element
    for number in numbers[1:]:
        # If the current number is smaller than the smallest_number, update smallest_number
        if number < smallest_number:
            smallest_number = number
    
    # Return the smallest_number
    return smallest_number

# Test the function with the input list
list1 = [5, 7, 9, 14, 10, 20, 4]
smallest_number = find_smallest_number(list1)

# Print the smallest number
print("The smallest number in the list is:", smallest_number)

